<!--fenbao/indent/indent.wxml-->
<view class="indent_all" hover-class="none" hover-stop-propagation="false">
    <image class="back" src="{{back}}" mode="widthFix" style="height:auto" lazy-load="false" binderror="" bindload="" />
      <view class="indent_content">

 <i-tabs current="{{ current }}" i-class="topbar" color="#dbaf67" bindchange="handleChange">
    <i-tab key="0" title="代付款"></i-tab>
    <i-tab key="1" title="代发货"></i-tab>
    <i-tab key="2" title="待收货"></i-tab>
     <i-tab key="3" title="已完成"></i-tab>
    <i-tab key="4" title="售后"></i-tab>
</i-tabs>
          <swiper class="tab-content" current="{{current}}" duration="300" bindchange="reLaunch" style="height:100vh;overflow:auto">
            <swiper-item  style="overflow:auto" >
                  <scroll-view scroll-y="true" class="scoll-h">
                            <!-- 000000000000000000000000000000000000000000000000000 -->


                      
        <!-- 订单列表 -->
        <view class="ddlist">
            <view class="dditem" wx:for="{{ddlist}}" wx:key="index">
                    <view class="dditem_top">
                        <view>{{item.sx}}</view>
                        <view class="flex3 fdsffsdf">
                            <view>订单号:{{item.ordernum}}</view>
                            <view>{{item.xdtime}}</view>
                        </view>
                    </view>
                    <view class="meddlew flex4">
                        <view class="dsib fdfwefg vret">
                            <image class="goossrc" src="{{item.goosimg}}" mode="widthFix" style="height:auto" lazy-load="false" binderror="" bindload="" />
                              
                        </view>
                        <view class="dsib vret fdsfhhgh">
                                <view>
                                    {{item.goostitle}}
                                </view>
                                <view class="hfiwee font_color_a">
                                    规格:{{item.guige}}
                                </view>
                                <view class="flex3 font_color_a">
                                    <view>
                                        x{{item.num}}
                                    </view>
                                    <view class="font_color_b">
                                        ￥：{{item.onlyprice}}
                                    </view>
                                </view>
                        </view>
                    </view>
                    <view class="bottoddlist flex3">
                        <view>   合计:{{item.num * item.onlyprice}}</view>
                        <view>
                              <i-button i-class="buttonstyle13 dsib  verm" class=" " shape="circle" bind:tap="yuyue">
                                                取消
                                        </i-button>
                              <i-button i-class="buttonstyle15 dsib  verm" class=" " shape="circle" bind:tap="yuyue">
                                                付款
                                        </i-button>
                        </view>
                         
                    </view>
            </view>
        </view>

 
                  </scroll-view>
            </swiper-item>
