<!--fenbao/share card/share card.wxml-->
<view class="share_all">
    <image class="share_back" src="{{my_back}}" mode="widthFix" style="height:auto" ></image>
<!-- 卡片 -->
    <view class="card">

            <view class="ssooo">
                   <image class="card_photo" src="{{photo}}" mode="widthFix" style="height:auto" ></image>
            </view>

   
        
        <view class="card_content" >
                <view class="card_name" >
                    DevinSteve
                    <image class="sex" src="{{sex}}" mode="widthFix" style="height:auto" ></image>
                </view>

                <view class="card_about">
                    我们一起撸起袖子~ 加油干！
                </view>

                <image class="card_ma" src="{{card_ma}}"  mode="widthFix" style="height:auto"/>
                  
        </view>
          
    </view>

    <!-- 其他 -->
    <view class="hiscup">
            <view class="cuptit fs54 flex3" >
                TA的俱乐部
                <view class="kaiguan">
                    只看他创建的
                    <!-- 开关按 -->
                    <kaiguan></kaiguan>
                </view>
            </view>

            <view class="cupdontai">
                    <view class="cupdo_list flex3" wx:for="{{cupdo_list}}" wx:key="index">
                          <view class="dsib verm fdsfsdf" >
                                <view class="first_" hover-class="none" hover-stop-propagation="false">
                                    {{item.tit}}
                                </view>

                                <view class="sed_ fs36" hover-class="none" hover-stop-propagation="false">
                                    已更新  <text class="kooks">{{item.num}}</text>   条
                                </view>
                     </view>
                        <view class="dsib verm">
                                 <image class="first_image dsds verm"  src="{{item.itemimg[0]}}" mode="widthFix" style="height:auto" />
                                 <image class="first_image dsds verm"  src="{{item.itemimg[1]}}" mode="widthFix" style="height:auto" />
                                   <image class="first_image dsds verm"  src="{{item.itemimg[2]}}" mode="widthFix" style="height:auto" />
                                  <image class="jianto verm" src="{{jiantou}}" mode="widthFix" style="height:auto" ></image>

                        </view>
                             
                     
                    </view>

            </view>

    </view>


</view>
